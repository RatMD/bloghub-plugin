{
    "name": "ratmd/bloghub-plugin",
    "type": "october-plugin",
    "version": "1.4.0",
    "description": "Extends RainLab's Blog extension with custom meta details, additional archives and more.",
    "keywords": [
        "october", 
        "octobercms", 
        "blog",
        "meta",
        "archives"
    ],
    "homepage": "https://octobercms.com/plugin/ratmd-bloghub",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "sam@rat.md",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@rat.md",
        "issues": "https://www.github.com/RatMD/bloghub-plugin/issues",
        "source": "https://www.github.com/RatMD/bloghub-plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 7.4 || >= 8.0",
        "composer/installers": "~1.0",
        "gregwar/captcha": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
